{
  "manifest_version": 3,
  "name": "SpeedyVideo",
  "version": "0.1.0",
  "description": "video speed controller",
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "index.html"
  }
}
